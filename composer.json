{
    "name": "sierratecnologia/integrations",
    "description": "Integrations de ServiÃ§os",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "integrations"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/integrations/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/integrations/issues",
        "source": "https://github.com/sierratecnologia/integrations",
        "docs": "https://github.com/sierratecnologia/integrations/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "aws/aws-sdk-php": "^3.0",
        "cache/filesystem-adapter": ">=1.0",
        "fideloper/proxy": ">=4.0",
        "geerlingguy/ping": "*",
        "google/photos-library": ">=1.4",
        "guzzlehttp/guzzle": "^6.0|^7.0|^8.0",
        "hubspot/hubspot-php": "^3.0",
        "league/climate": ">=3.4",
        "league/csv": "^9.0|^8.0|^7.1",
        "league/flysystem-aws-s3-v3": "*",
        "league/pipeline": "*",
        "league/plates": "^3.1|^4.0",
        "m4tthumphrey/php-gitlab-api": "*",
        "raiym/instagram-php-scraper": ">=0.9.10",
        "phpfastcache/phpfastcache": "^8.1|^9.0",
        "ricasolucoes/atlassian": "dev-master",
        "sabre/xml": "*",
        "sierratecnologia/muleta": "dev-master",
        "srmklive/flysystem-dropbox-v2": "*",
        "texthtml/docker-links": "*"
    },
    "autoload": {
        "psr-4": {
            "Integrations\\": "src/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.5",
        "vimeo/psalm": "^4.0",
        "phpro/grumphp": ">=1.3"
    },
    "repositories": [{"type": "path","url":"../**","options":{"symlink": true}}],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpro/grumphp": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Integrations\\IntegrationsProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
